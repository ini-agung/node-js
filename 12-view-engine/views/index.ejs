<table class="table table-hover mt-5">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
            <th scope="col">Handle</th>
        </tr>
    </thead>
    <tbody>
        <% if (mahasiswa.length>0) { %>
            <%mahasiswa.forEach(mhs => { %>
                <tr>
                    <th scope="row">1</th>
                    <td>
                        <%= mhs.nama %>
                    </td>
                    <td>
                        <%= mhs.email %>
                    </td>
                    <td>
                        <%= mhs.phone %>
                    </td>
                </tr>
                <% }); %>
                    <% }else{ %>
                        <tr>
                            <td colspan="3">
                                <div class="btn btn-danger ">Data Tidak Ditemukan</div>
                            </td>
                        </tr>
                        <%}%>
    </tbody>
</table>